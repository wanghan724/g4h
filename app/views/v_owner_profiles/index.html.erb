<p id="notice"><%= notice %></p>

<h1>V Owner Profiles</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Email</th>
      <th>Contact number</th>
      <th>About</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @v_owner_profiles.each do |v_owner_profile| %>
      <tr>
        <td><%= v_owner_profile.first_name %></td>
        <td><%= v_owner_profile.last_name %></td>
        <td><%= v_owner_profile.email %></td>
        <td><%= v_owner_profile.contact_number %></td>
        <td><%= v_owner_profile.about %></td>
        <td><%= link_to 'Show', v_owner_profile %></td>
        <td><%= link_to 'Edit', edit_v_owner_profile_path(v_owner_profile) %></td>
        <td><%= link_to 'Destroy', v_owner_profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New V Owner Profile', new_v_owner_profile_path %>
